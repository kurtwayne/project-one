<h1>All Wikis</h1>

<% @wikis.each do |wiki| %>
  <div class="row">
    <div class="col-md-8">
      <h4 class="media-heading">
        <p><%= link_to wiki.title, wiki %></p>
      </h4>
    </div>
<% end %>

<div class="row">
  <div class="col-md-4">
    <%= link_to "Create New Wiki", new_wiki_path(@wiki), class: 'btn btn-success' %>
  </div>
</div>
</div>
